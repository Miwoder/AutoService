<Window x:Class="AutoService.EditPrice"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoService"
        mc:Ignorable="d"
        Title="EditPrice" Height="150" Width="235"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <StackPanel>
            <TextBox Text="{Binding editPrice}" Margin="5,20" x:Name="editPriceTB" Width="100"/>
            <Button Command="{Binding SavePrice}" Content="OK" Width="60" IsDefault="True"/>
        </StackPanel>
    </Grid>
</Window>
